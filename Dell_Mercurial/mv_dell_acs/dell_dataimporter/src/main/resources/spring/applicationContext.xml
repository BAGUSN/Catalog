       default-init-method="init" default-destroy-method="destroy"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <import resource="applicationContext-dell-core.xml"/>

    <import resource="applicationContext-core-services-texttemplate.xml"/>
    <import resource="applicationContext-core-services-i18n.xml"/>

	<!--  job recover tasks -->
	<import resource="applicationContext-recover-tasks.xml"/>

    <!-- web -->
    <import resource="applicationContext-web-core.xml"/>
    <import resource="applicationContext-web.xml"/>

    <!-- jobs -->
    <import resource="applicationContext-new-dataimport.xml"/>
    <import resource="applicationContext-new-dataimport-jobs.xml"/>
    <import resource="applicationContext-new-dataimport-download-jobs.xml"/>

    <!-- security -->
    <import resource="applicationContext-security.xml"/>
    
</beans>
