	<target name="build_clean" description="Clean and Build the dell_dataimporter web application.">
		<antcall target="clean"/>
		<antcall target="build"/>
	</target>

	<target name="debug_build_clean" description="Clean and Debug Build the dell_dataimporter web application.">
