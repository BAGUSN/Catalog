fo:table-row>											<!-- EDITED -->											<xsl:if test="initialTransaction">												<fo:table-row>													<fo:table-cell margin-left="5px">														<fo:block line-height="12px" font-size="8px" keep-together="always" text-align="left" width="50%" padding="5px">															<fo:table>																<fo:table-body>																	<fo:table-row>																		<fo:table-cell border-style="solid" border-color="black" border-width="1px" padding="4px">																			<fo:block>																				<fo:table>																					<fo:table-column column-width="17%"/>																					<fo:table-column/>																					<fo:table-body>																						<fo:table-row>																							<fo:table-cell>																								<fo:block text-align="left" margin-left="5px">Name: </fo:block>																							</fo:table-cell>																							<fo:table-cell border-bottom="1px solid #000000">																								<fo:block>																									<xsl:value-of select="initialTransaction/businessAddress/businessName"/>																								</fo:block>																							</fo:table-cell>																						</fo:table-row>																					</fo:table-body>																				</fo:table>																			</fo:block>																			<fo:block text-align="left" margin-left="8px">																				<fo:inline>Address: </fo:inline>																				<fo:inline text-decoration="underline">																					<xsl:value-of select="initialTransaction/businessAddress/street1"/>																					<xsl:if test="string(initialTransaction/businessAddress/street2)">,																				</xsl:if>																					<xsl:value-of select="initialTransaction/businessAddress/street2"/>																					<xsl:if test="string(initialTransaction/businessAddress/city)">,																				</xsl:if>																					<xsl:value-of select="initialTransaction/businessAddress/city"/>													