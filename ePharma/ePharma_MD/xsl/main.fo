nt-size="8px">																		<fo:table>																			<fo:table-column column-width="60%"/>																			<fo:table-column column-width="40%"/>																			<fo:table-body>																				<fo:table-row>																					<fo:table-cell>																						<fo:block border-bottom="1px solid #000000" text-align="center" font-size="8px" margin-top="20px">																							<xsl:value-of select="businessName"/>																						</fo:block>																					</fo:table-cell>																					<fo:table-cell>																						<fo:block font-style="italic" font-size="8px" margin-top="20px">(Repackager&#8216;s Name)</fo:block>																					</fo:table-cell>																				</fo:table-row>																			</fo:table-body>																		</fo:table>																	</fo:block>																</fo:table-cell>																<fo:table-cell>																	<fo:block>																		<fo:external-graphic src="xsl/draft.gif"/>																	</fo:block>																</fo:table-cell>															</fo:table-row>															<fo:table-row>																<fo:table-cell>																	<fo:block text-align="center" font-size="8px" font-weight="bold" text-decoration="italic">PRESCRIPTION (LEGEND) DRUG PEDIGREE</fo:block>																</fo:table-cell>																<fo:table-cell>																	<fo:block text-align="center" font-size="8px">																		<fo:inline white-space-collapse="false"> </fo:inline>																	</fo:block>																</fo:table-cell>															</fo:table-row>															<fo:table-row>																<fo:table-cell number-columns-spanned="2">																	<fo:block font-size="8px" font-weight="bold" text-decoration="italic" text-align="left">														History of Drug Sales and Distributions for a Repackaged Prescription Drug</fo:block>																</fo:table-cell>															</fo:table-row>														</fo:table-body>					