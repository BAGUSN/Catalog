l>																<fo:table-cell border-style="solid" border-color="black" border-width="1px">																	<fo:block line-height="12px" font-size="8px" font-weight="bold">Quantity</fo:block>																</fo:table-cell>																<fo:table-cell border-style="solid" border-color="black" border-width="1px" font-size="8px" font-weight="bold">																	<fo:block line-height="12px" space-before.opximum="1.5px" space-after.opximum="1.5px" keep-together="always" font-size="8px">Unique Tracking# (RFID/UPC)</fo:block>																</fo:table-cell>															</fo:table-row>															<xsl:for-each select="itemInfo">																<fo:table-row>																	<fo:table-cell border-style="solid" border-color="black" border-width="1px" margin-right="5px" margin-left="5px" margin-bottom="5px" margin-top="5px">																		<fo:block line-height="12px" font-size="8px" space-before.opximum="1.5px" space-after.opximum="1.5px" keep-together="always">																			<xsl:value-of select="lot"/>																		</fo:block>																	</fo:table-cell>																	<fo:table-cell border-style="solid" border-color="black" border-width="1px" margin-right="5px" margin-left="5px" margin-bottom="5px" margin-top="5px">																		<fo:block line-height="12px" font-size="8px" space-before.opximum="1.5px" space-after.opximum="1.5px" keep-together="always">																			<xsl:value-of select="quantity"/>																		</fo:block>																	</fo:table-cell>																	<fo:table-cell border-style="solid" border-color="black" border-width="1px" padding="3px">																		<fo:block line-height="12px" font-size="8px">																			<fo:inline>																				<xsl:value-of select="itemSerialNumber"/>																			</fo:inline>																		</fo:block>																	</fo:table-cell>																</fo:table-row>															</xsl:for-each>														</fo:table-body>										